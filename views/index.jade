//dynamic content in the indiv. partial file
extends layout.jade

block content
  .container-fluid
  h1= title
  //- p  #{title}
  a(href='/addUser') Click This Link to Add a User
  
  table.table
    thead
      tr
        th Name
        th Email
        th Location
        th Phone Number
        th Facebook
    tbody#addContact
      each contact in contacts
        tr
          td= contact.name
          td= contact.email
          td= contact.loc
          td= contact.phone
          td= contact.face

block scripts
  script(src='js/main.js')
